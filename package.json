{
  "name": "naturheilpraxis",
  "version": "0.1.0",
  "description": "Mit dieser App können Heilpraktiker Leistungen für Patienten erfassen, um ihnen diese in Rechnung stellen zu können.",
  "keywords": [
    "naturheilpraxis",
    "heilpraktiker",
    "rechnung"
  ],
  "author": "Falko Schumann",
  "homepage": "https://github.com/falkoschumann/naturheilpraxis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/falkoschumann/naturheilpraxis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falkoschumann/naturheilpraxis.git"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=24.0.0"
  },
  "main": "./build/main/index.js",
  "scripts": {
    "start": "electron-vite preview --outDir=build",
    "dev": "electron-vite dev --outDir=build --watch",
    "build": "tsc --build && electron-vite build --outDir=build",
    "build:unpack": "electron-builder --dir",
    "build:electron": "electron-builder",
    "build:win": "electron-builder --win --x64",
    "build:mac": "electron-builder --mac --arm64",
    "build:linux": "electron-builder --linux --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@js-temporal/polyfill": "0.5.1",
    "@muspellheim/shared": "0.17.0",
    "@softarc/sheriff-core": "0.19.6",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "3.13.18",
    "@types/node": "24.10.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.17",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "bootstrap5-tags": "1.7.16",
    "cloudevents": "10.0.0",
    "electron": "40.0.0",
    "electron-builder": "26.4.0",
    "electron-devtools-installer": "4.0.0",
    "electron-vite": "5.0.0",
    "eslint": "9.39.2",
    "eslint-plugin-headers": "1.3.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "flux-standard-action": "2.1.2",
    "globals": "17.0.0",
    "prettier": "3.8.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0",
    "sass": "1.97.2",
    "type-fest": "5.4.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1",
    "vitest": "4.0.17"
  }
}
