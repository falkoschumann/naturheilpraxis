{
  "name": "naturheilpraxis",
  "productName": "Naturheilpraxis",
  "version": "0.1.0",
  "description": "Mit dieser App können Heilpraktiker Leistungen für Patienten erfassen, um ihnen diese in Rechnung stellen zu können.",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "./dist/main/index.js",
  "scripts": {
    "start": "electron-vite preview --outDir=dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "electron-vite dev --outDir=dist --watch",
    "build": "npm run typecheck && electron-vite build --outDir=dist",
    "package": "npm run build &&  electron-forge package",
    "make": "npm run build &&  electron-forge make",
    "publish": "npm run build &&  electron-forge publish",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false"
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.8.1",
    "@electron-forge/maker-deb": "7.8.1",
    "@electron-forge/maker-dmg": "7.8.1",
    "@electron-forge/maker-rpm": "7.8.1",
    "@electron-forge/maker-squirrel": "7.8.1",
    "@electron-forge/maker-zip": "7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "7.8.1",
    "@electron-forge/plugin-fuses": "7.8.1",
    "@electron/fuses": "1.8.0",
    "@eslint/js": "9.30.1",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/node": "22.13.14",
    "electron": "37.2.0",
    "electron-vite": "3.1.0",
    "eslint": "9.30.1",
    "eslint-plugin-headers": "1.3.3",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vite": "6.3.5"
  },
  "keywords": [
    "naturheilpraxis",
    "heilpraktiker",
    "rechnung"
  ],
  "author": {
    "name": "Falko Schumann",
    "email": "falko.schumann@muspellheim.de"
  },
  "license": "MIT",
  "homepage": "https://github.com/falkoschumann/naturheilpraxis",
  "bugs": {
    "url": "https://github.com/falkoschumann/naturheilpraxis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falkoschumann/naturheilpraxis.git"
  }
}
